{
  "name": "@quantified-me/backend",
  "version": "0.1.0",
  "description": "Backend API for Quantified Me",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.4.1",
    "zod": "^3.22.4",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "@aws-sdk/client-s3": "^3.504.0",
    "@aws-sdk/client-dynamodb": "^3.504.0",
    "@aws-sdk/client-sqs": "^3.504.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.504.0",
    "@anthropic-ai/sdk": "^0.17.0"
